<view class="cml-checkbox  cml-base cml-view cml-5277510b">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-5277510b" onTap="_cmlEventProxy" data-eventtap="changeCheck">
    <view class="cml-checkbox-check  cml-base cml-view cml-5277510b" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-5277510b">
        <image class="inner-icon  cml-base cml-image cml-5277510b" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-5277510b">{{label}}</text>
  </view>
</view>