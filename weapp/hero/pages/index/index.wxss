.page-section-spacing {
  background: rgba(0, 0, 0, 0.3);
  width: 100%;
  color: #fff;
  box-shadow: 0 0.01rem 0.18rem rgba(0, 0, 0, 0.6);
}

.scroll-view_H {
  white-space: nowrap;
}

.scroll-view-item_H {
  display: inline-block;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 10pt;
}

.navItem {
  text-align: center;
  padding: 0 10px;
  width: 100%;
  height: 100%;
  display: block;
}

.active {
  background: transparent url(data:image/png;base64,) no-repeat;
  background-size: contain;
  background-position: bottom center;
}

.heroList {
  width: 100%
}

.heroItem {
  width: 100%;
  box-sizing: border-box;
  padding: 5pt 5pt;
}

.heroItem .hItem {
  width: 45%;
  height: 180pt;
  background: transparent;
  margin: 5pt;
}

.heroItem .hItem:nth-of-type(2n) {
  float: right
}

.heroItem .hItem:nth-of-type(2n-1) {
  float: left
}

.hItemimg {
  width: 100%;
  height: 100%;
}