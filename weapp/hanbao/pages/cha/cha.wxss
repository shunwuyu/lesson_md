.bw_mapControler__btn {
  color: rgba(0,0,0,0.2);
  border: 1px solid rgba(0,0,0,0.2);
  font-size: 10pt;
  padding: 6rpx 16rpx;
  border-radius: 16rpx;
  background-color: #fff;
}
.bm_mapController__bd {
  height: 300rpx;
  position: relative;
  transition: all 1s;
}
.bm_mapController__bd-hide {
  height: 0;
}
.bm_mapController__bd map {
  width: 100%;
  height: 100%;
}
.bm_mapController__switch {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  font-size: 10pt;
}

.bm_mapController__switch i.more {
  margin-left: 10rpx;
  width: 25rpx;
  height: 25rpx;
  vertical-align: middle;
  transform: rotateZ(-90deg);
}