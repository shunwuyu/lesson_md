/* pages/cha/cha.wxss */
.page__bd {
  padding: 30rpx;
}
.bw_mapControler__hd {
  display: flex;
  align-items: center;
}
.bw_mapController__title {
  position: relative;
  padding-left: 54rpx;
  /* line-height: rpx; */
  /* padding-right: 80rpx; */
  flex: 1;
}
.bw_mapController__title::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 54rpx;
  height: 54rpx;
  /* background-color: red; */
  background: url(data:image/png;base64,);
  background-size: 100%;
}
i.more {
  display: inline-block;
  width: 54rpx;
  height: 54rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  vertical-align: middle;
}
